{
  "name": "madewithlove/export",
  "description": "Interfaces and basic implementations for file exports from a web application",
  "keywords": [
    "csv",
    "export",
    "download",
    "response",
    "extendable",
    "reusable"
  ],
  "homepage": "https://github.com/madewithlove/export",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Van De Vreken",
      "email": "hannes@mwl.be",
      "role": "maintainer"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "league/csv": "^8.0",
    "phpunit/phpunit": "^4.8",
    "symfony/http-foundation": "^2.6|^3.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Madewithlove\\Export\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Madewithlove\\Export\\Unit\\": "tests/unit/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs": "php-cs-fixer"
  }
}
